{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb-base",
        "plugin:react/all"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off"
    },
    "ignorePatterns": [
        "**/build/*",
        "**/coverage/*"
    ]
}